@Article{	  zhu199619,
  author	= {Zhu, Hong-jiang and Wolfgang Schilling},
  title		= {Simulation errors due to insufficient temporal rainfall
		  resolution -- Annual combined sewer overflow},
  journal	= {Atmospheric Research},
  year		= {1996},
  volume	= {42},
  pages		= {19 - 32},
  number	= {1-4},
  abstract	= {In urban hydrology the continuously recorded rainfall data
		  is digitized into constant rain intensity with time steps
		  between 1 min and 1 h. If the time steps are chosen too
		  large, short duration peak rainfall intensities are
		  filtered. As a consequence peak flow rates are
		  underestimated. This error is amplified if overflow volumes
		  from combined sewer systems are calculated. This paper
		  presents a method to predict errors of calculated mean
		  annual overflow volume due to coarse temporal resolution of
		  rainfall data that is used as input to rainfall-runoff
		  models. Errors are quantified by comparison of overflow
		  volumes based on rain data series with different temporal
		  resolution. Two rain data series (Fehraltorf/Switzerland
		  and Odense/Denmark) are used together with synthetic
		  combined sewer systems to compute errors of mean annual
		  overflow volume as a function of: - - the time resolution
		  of the rain data;- - the critical flow of an overflow
		  weir;- - the storage volume of an overflow tank; and- - the
		  inflow to an overflow weir or tank. The procedure is
		  verified for the sewer system of Fehraltorf, Switzerland,
		  located in the vicinity of Z\"{u}rich, using local rainfall
		  data. However, results showed that prediction of overflow
		  errors due to rain data resolution in Fehraltorf using the
		  Danish rainfall data is more difficult. Apparently, the
		  pattern of the temporal rainfall variation of the two data
		  series are too different to derive error prediction rules
		  that are generally applicable for all rainfall regimes.},
  doi		= {DOI: 10.1016/0169-8095(95)00050-X},
  issn		= {0169-8095},
  url		= {http://www.sciencedirect.com/science/article/B6V95-3VVT43D-3/2/ba220890cc7c39f8
		  9d8a57d5fab22d18}
}
