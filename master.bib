@Article{	  rauch19991265,
  author	= {Wolfgang Rauch and Poul Harremo\"{e}s},
  title		= {Genetic algorithms in real time control applied to
		  minimize transient pollution from urban wastewater
		  systems},
  journal	= {Water Research},
  year		= {1999},
  volume	= {33},
  pages		= {1265 - 1277},
  number	= {5},
  abstract	= {Real time control aims at optimization of the urban
		  wastewater system performance under dynamic loading from
		  rain. This paper presents a novel approach to control the
		  whole system: sewer system, treatment plant and receiving
		  water with the aim to achieve minimum effects of pollution.
		  The application of nonlinear model predictive control by
		  means of a genetic algorithm reveals excellent results with
		  hypothetical problem sets. The methodology makes it
		  possible to optimize the system performance directly with
		  respect to water quality parameters and to avoid the
		  traditional indirect and artificial performance criteria,
		  such as permissible annual overflow volume. The relevance
		  of this novel approach is illustrated by the fact that no
		  stringent correlation has been found in the investigation
		  between the reduction of overflow volume and the increase
		  of oxygen concentration in the receiving water.},
  doi		= {DOI: 10.1016/S0043-1354(98)00304-2},
  issn		= {0043-1354},
  keywords	= {genetic algorithms},
  url		= {http://www.sciencedirect.com/science/article/B6V73-3Y51DS7-N/2/a28b84c1dc160ffa
		  48ecfeb749c030d1}
}

