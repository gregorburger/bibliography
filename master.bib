@Article{	  mitchell2008782,
  author	= {V. G. Mitchell and D. T. McCarthy and A. Deletic and T. D.
		  Fletcher},
  title		= {Urban stormwater harvesting - sensitivity of a storage
		  behaviour model},
  journal	= {Environmental Modelling \& Software},
  year		= {2008},
  volume	= {23},
  pages		= {782 - 793},
  number	= {6},
  abstract	= {The harvesting of urban stormwater to supply non-potable
		  water demands is emerging as a viable option, amongst
		  others, as a means to augment increasingly stressed urban
		  water supply systems. This paper investigates the
		  sensitivity of an urban stormwater harvesting system's
		  capacity-yield-reliability relationship to variations in
		  the behaviour modelling method used, focusing on the
		  storage and demand components of a single reservoir system.
		  The aim is to enhance our understanding of the appropriate
		  computational method for assessing such volumetric
		  reliability/storage capacity relationships. Four reference
		  scenarios were developed, based on two different climates
